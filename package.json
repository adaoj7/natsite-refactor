{
  "name": "natsite-refactor",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "nodemon api/index.js -w api -w src",
    "pm2-runtime": "pm2-runtime api/index.js --name natsite -- --serve-dist",
    "tailwind": "npx tailwindcss -i ./src/index.css -o ./dist/output.css --watch",
    "tailwind:nowatch": "npx tailwindcss -i ./src/index.css -o ./dist/output.css",
    "build": "tsc -b && vite build && npm run tailwind:nowatch",
    "build-vite": "vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite",
    "test": "vitest",
    "all": "concurrently npm:dev npm:tailwind",
    "pm2-build": "npm run build && npm run pm2-runtime",
    "stop": "pm2 delete natsite",
    "seed": "node api/dbscripts/seed.js",
    "sync": "node api/dbscripts/sync.js",
    "docker:build": "docker build --pull --rm -f \"Dockerfile\" -t adamj7/natsite-refactor:latest \".\"",
    "docker:push": "docker push adamj7/natsite-refactor:latest",
    "docker": "npm run build && npm run docker:build && npm run docker:push"
  },
  "dependencies": {
    "@auth0/auth0-react": "^2.2.4",
    "@headlessui/react": "^2.1.2",
    "@reduxjs/toolkit": "^2.2.7",
    "@tanstack/react-query": "^5.51.1",
    "axios": "^1.7.3",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "formik": "^2.4.6",
    "libphonenumber-js": "^1.11.7",
    "morgan": "^1.10.0",
    "nodemon": "^3.1.4",
    "pm2": "^5.4.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-phone-number-input": "^3.4.5",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.26.0",
    "sequelize": "^6.37.3",
    "stripe": "^16.11.0",
    "swiper": "^11.1.14",
    "vite-express": "^0.17.0",
    "yup": "^1.4.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.9",
    "@tanstack/eslint-plugin-query": "^5.51.1",
    "@testing-library/dom": "^10.3.2",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@types/express": "^4.17.21",
    "@types/node": "^22.5.0",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@typescript-eslint/eslint-plugin": "^7.13.1",
    "@typescript-eslint/parser": "^7.13.1",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "concurrently": "^8.2.2",
    "daisyui": "^4.12.10",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.4",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.8",
    "tailwindcss": "^3.4.13",
    "typescript": "^5.2.2",
    "vite": "^5.3.1",
    "vitest": "^2.0.3"
  }
}
